<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Amasty_VictoriaModule:etc/cron_groups.xsd">
    <group id="victoriamodule_email">
        <schedule_generate_every>10</schedule_generate_every>
        <schedule_ahead_for>5</schedule_ahead_for>
        <schedule_lifetime>15</schedule_lifetime>
        <history_cleanup_every>10</history_cleanup_every>
        <history_success_lifetime>60</history_success_lifetime>
        <history_failure_lifetime>430</history_failure_lifetime>
        <use_separate_process>0</use_separate_process>
    </group>
</config>
